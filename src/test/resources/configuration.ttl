@prefix fcage: <https://w3id.org/fcage/> .
@prefix deer: <https://w3id.org/deer/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix geos: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

#:reader
 #a deer:FileModelReader ;
# deer:fromPath "F:/ontologyPGmatching/matching/Sparql.n3";
 #fcage:hasOutput :ontologyMatcher;
#.

:reader
 a deer:FileModelReader ;
 deer:fromPath "jsontordfoutput.ttl";
 fcage:hasOutput :kgmatcher ; 
.

:kgmatcher
  a deer:KGMatchingOperator ;
  fcage:hasInput :reader ;
  deer:matchingApproach "LIMES" ;
 . 


:ontologyMatcher
 a deer:OntologyMatchingOperator ;
 deer:typeOfMap "Classes";
 fcage:hasInput :kgmatcher ;
.

 
:writer
 a deer:StdOutModelWriter ;
 fcage:hasInput :ontologyMatcher ;
.
